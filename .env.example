# Clerk Configuration
CLERK_PUBLISHABLE_KEY=pk_test_your_key_here
CLERK_SECRET_KEY=sk_test_your_key_here
CLERK_JWT_KEY=your_jwt_key_here

# FastAPI Configuration
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=True
CORS_ORIGINS=["http://localhost:3000","http://localhost:5173"]

# LangSmith Configuration
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=chronochat

# AI Provider Configuration
AI_PROVIDER=gemini  # Options: openai, gemini
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_gemini_api_key_here

# Model Configuration
GEMINI_MODEL=gemini-2.0-flash-exp  # or gemini-1.5-pro, gemini-1.5-flash
OPENAI_MODEL=gpt-4  # or gpt-3.5-turbo, gpt-4-turbo

# PostgreSQL Configuration (for user data, analytics, session data)
DATABASE_URL=postgresql://chronochat:chronochat@postgres:5432/chronochat

# MongoDB Configuration (for chat conversations and messages)
MONGODB_URL=mongodb://chronochat:chronochat@mongodb:27017
MONGODB_DB_NAME=chronochat

# Redis Configuration (for caching and rate limiting)
REDIS_URL=redis://redis:6379
